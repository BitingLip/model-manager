# Model Manager Database Configuration
# Dedicated PostgreSQL database for model registry and worker management

# Database connection settings
MODEL_DB_HOST=localhost
MODEL_DB_PORT=5432
MODEL_DB_NAME=bitinglip_models
MODEL_DB_USER=model_manager
MODEL_DB_PASSWORD=model_manager_2025!

# Connection pool settings
MODEL_DB_POOL_SIZE=10
MODEL_DB_MAX_OVERFLOW=20
MODEL_DB_POOL_TIMEOUT=30

# Model management settings
MODEL_REGISTRY_CACHE_TTL=300
MODEL_DOWNLOAD_TIMEOUT=3600
MODEL_HEARTBEAT_INTERVAL=30

# Logging and monitoring
MODEL_DB_LOG_LEVEL=INFO
MODEL_DB_SLOW_QUERY_THRESHOLD=1.0
MODEL_DB_CONNECTION_LOGGING=false

# Backup and maintenance
MODEL_DB_AUTO_BACKUP=true
MODEL_DB_BACKUP_INTERVAL_HOURS=6
MODEL_DB_VACUUM_SCHEDULE="0 2 * * *"
